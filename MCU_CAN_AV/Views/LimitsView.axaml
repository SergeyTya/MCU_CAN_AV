<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="clr-namespace:MCU_CAN_AV.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MCU_CAN_AV.Views.LimitsView">


	<UserControl.DataContext>
		<vm:LimitsViewModel/>
	</UserControl.DataContext>
	
 <StackPanel Orientation="Vertical" Margin="0,10,0,0">
	 <StackPanel Orientation="Horizontal" Spacing="10" VerticalAlignment="Center">
		 <TextBlock Text="Voltage limit" Width="150" TextAlignment="Center" />
		 <TextBox  Width="150" Text="{Binding VoltageLim}"  />
		 <CheckBox IsChecked="{Binding VoltageLim_enable}"  VerticalAlignment="Center"/>
	 </StackPanel>
	 <StackPanel Orientation="Horizontal" Spacing="10" VerticalAlignment="Center">
		 <TextBlock Text="Speed limit" Width="150" TextAlignment="Center" />
		 <TextBox  Width="150" Text="{Binding SpeedLim}"  />
		 <CheckBox IsChecked="{Binding SpeedLim_enable}"  VerticalAlignment="Center"/>
	 </StackPanel>
	 
	 
 </StackPanel>
	
</UserControl>
