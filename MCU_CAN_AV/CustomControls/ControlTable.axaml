<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:cc="using:MCU_CAN_AV.CustomControls"
             x:Class="MCU_CAN_AV.CustomControls.ControlTable"
			 x:DataType="cc:ControlTable"
			 x:CompileBindings="True"
			 >
	<ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Visible" Width="{Binding .}" Background="Black">
		<Panel Margin="0,0,16,0" Background="DimGray" Width=" ">
			<Grid  Name="Grid_main" ShowGridLines="False" VerticalAlignment="Stretch"> </Grid>
		</Panel>
	</ScrollViewer>

</UserControl>
